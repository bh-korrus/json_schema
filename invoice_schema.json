{
  "title": "Purchase Order Data",
  "description": "Extracted fields from a purchase order PDF",
  "type": "object",
  "properties": {
    "po_number": {
      "type": "string",
      "description": "The unique purchase order identifier"
    },
    "entity_name": {
      "type": "string",
      "description": "Name of the person or business placing the purchase order"
    },
    "date": {
      "type": "string",
      "format": "date",
      "description": "Purchase order date in YYYY-MM-DD"
    },
    "total": {
      "type": "number",
      "description": "Total amount due"
    },
    "line_items": {
      "type": "array",
      "description": "List of items on the purchase order",
      "items": {
        "type": "object",
        "properties": {
          "sku": {
            "type": "string",
            "description": "Stock keeping unit"
          },
          "description": {
            "type": "string",
            "description": "Item description"
          },
          "count": {
            "type": "integer",
            "description": "Quantity ordered"
          }
        },
        "required": ["sku", "description", "count"]
      }
    }
  },
  "required": ["po_number", "entity_name", "date", "total", "line_items"]
}
